    }

    VideoShareOverlay.prototype = {
        // Set a method on this object as the event handler.
        handleEvent: function(obj, eventName, handlerName) {
            var self = this;
            obj.addEventListener(eventName, function(e) {
                self[handlerName](e);
            });
        },

